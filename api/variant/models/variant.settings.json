{
  "kind": "collectionType",
  "collectionName": "variants",
  "info": {
    "name": "Variant",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "size": {
      "type": "string",
      "required": false
    },
    "color": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "qty": {
      "type": "integer"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "style": {
      "type": "string",
      "required": false
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "product": {
      "model": "product",
      "via": "variants"
    },
    "colorLabel": {
      "type": "enumeration",
      "enum": [
        "black",
        "white",
        "green",
        "red",
        "tan"
      ]
    }
  }
}
